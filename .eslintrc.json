{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "react/prop-types": 0,
    "react/jsx-filename-extension": [
      1, {
        "extensions": [".js", ".jsx"]
      }
    ]
  },
  "settings": {
    "import/core-modules": [
      "react"
    ]
  },
  "globals": {
    "graphql": true
  }
}
